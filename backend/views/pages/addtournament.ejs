<%- include('../template/head')-%>
<%- include('../template/navTournament')-%>
<div class="row">
    <form class="col s10 push-s1" action="http://localhost:3000/api/tournament/addTournament" method="POST">
        <div class="row">
            <div class="input-field col s12">
                <input name="name" id="name" type="text" class="validate">
                <label for="name">Name</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input  name="address.address" id="address.address" type="text" class="validate">
                <label for="address.address">Address</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input  name="address.city" id="address.city" type="text" class="validate">
                <label for="address.city">City</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input  name="address.code" id="address.code" type="number" class="validate">
                <label for="address.code">ZIP code</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="team" id="team" type="text" class="validate">
                <label for="team">Team</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="date" type="text" class="datepicker">
                <label for="date">Date</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="time" type="text" class="timepicker">
                <label for="time">Time</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="numberOfTour" id="numberOfTour" type="text" class="validate">
                <label for="numberOfTour">Number of tour</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="numberOfTeam" id="numberOfTeam" type="text" class="validate">
                <label for="numberOfTeam">Number of team</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="numberOfGroup" id="numberOfGroup" type="text" class="validate">
                <label for="numberOfGroup">Number of group</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="numberofPoint" id="numberofPoint" type="text" class="validate">
                <label for="numberofPoint">Number of point</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="timeBetweenMatch" id="timeBetweenMatch" type="text" class="validate">
                <label for="timeBetweenMatch">Time between match</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input name="timeOfMatch" id="timeOfMatch" type="text" class="validate">
                <label for="timeOfMatch">Time between match</label>
            </div>
        </div>
    <div class="row">
        <div class="">
            <input type="submit" id="submit" class="waves-effect waves-light btn btnIndex btnplein" value="Add">
        </div>
    </div>
    <div class="row">
        <div class="">
           
        </div>
    </div>
</div>

</form>
<%- include('../template/footer')-%>